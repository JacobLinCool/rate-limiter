		"lint-staged": "^13.1.0",
		"prettier": "^2.8.1",
		"serve": "^14.1.2",
		"ts-jest": "^29.0.3",
		"tsup": "^6.5.0",
		"tsx": "^3.12.1",
		"typedoc": "^0.23.23",
		"typescript": "^4.9.4"
	},
	"lint-staged": {
		"*.{ts,js,json,yaml,yml}": [
			"prettier --write"
